{
  "family": "ehr-app-test",
  "containerDefinitions": [
    {
      "name": "ehr-app-test",
      "image": "hspconsortium/ehr-app:0.2.0-SNAPSHOT",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "memoryReservation": 100,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ehr-app-test",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true
    }
  ]
}
