{
  "family": "patient-data-manager-prod",
  "containerDefinitions": [
    {
      "name": "patient-data-manager-prod",
      "image": "hspconsortium/patient-data-manager:1.2.0-SNAPSHOT",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8096,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "memoryReservation": 100,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/patient-data-manager-prod",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true
    }
  ]
}
